{
  "name": "vue-app2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env CODE_ENV=dev vue-cli-service serve",
    "build": "cross-env CODE_ENV=prod vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "eslint --fix --ext .js src",
    "deploy_dev": "cross-env CODE_ENV=dev vue-cli-service build",
    "deploy_beta": "cross-env CODE_ENV=beta vue-cli-service build",
    "deploy_prod": "cross-env CODE_ENV=prod vue-cli-service build"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "element-ui": "^2.8.2",
    "vue": "^2.6.8",
    "vue-meta": "^2.0.4",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^8.1.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.10"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "lint-staged": {
    "*.js,*.vue": [
      "lint:fix",
      "git add"
    ]
  }
}
